{
    "name": "rahimi-ali/pulse-slow-mongo-queries",
    "version": "1.0.0",
    "description": "A Laravel Pulse card for slow MongoDB queries",
    "keywords": ["laravel", "pulse", "mongodb"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ali Rahimi",
            "email": "arahimihajiabadi@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-mongodb": "*",
        "illuminate/support": "*",
        "laravel/pulse": "^1.0.0@beta"
    },
    "require-dev": {
        "orchestra/testbench": "^8",
        "mockery/mockery": "^1.5.0",
        "phpunit/phpunit": "^10"
    },
    "autoload": {
        "psr-4": {
            "RahimiAli\\Pulse\\SlowMongoQueries\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RahimiAli\\Pulse\\SlowMongoQueries\\SlowMongoQueriesServiceProvider"
            ]
        }
    }
}